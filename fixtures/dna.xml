              <render_choice>
                <response_label ident="6262">
                  <material>
                    <mattext texttype="text/html">Amino acids make up proteins, proteins make up RNA, and RNA makes up DNA.</mattext>
                  </material>
                </response_label>
                <response_label ident="2954">
                  <material>
                    <mattext texttype="text/html">Genes specify the sequence of proteins which are assembled by RNA.</mattext>
                  </material>
